{
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "fetch": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "quotes": [2, "single"],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  }
}
